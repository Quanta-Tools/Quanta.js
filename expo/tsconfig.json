{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "declaration": true,
    "declarationDir": "dist",
    "outDir": "dist",
    "preserveSymlinks": true,
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "rootDir": "src"
  },
  "include": ["src", "test"],
  "exclude": ["node_modules", "dist"]
}
