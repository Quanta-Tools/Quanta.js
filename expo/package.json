    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.1",
    "fs-extra": "^11.3.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.6",
    "rollup": "^4.39.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.3.1",
    "typescript": "^5.8.3",
    "babel-jest": "^29.7.0",
    "babel-preset-expo": "^13.1.3"
