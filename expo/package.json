{
  "name": "expo-quanta",
  "version": "0.0.4",
  "description": "Quanta.js/Expo: Expo SDK for Quanta Tools",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:web": "rollup -c; node post-build.js",
