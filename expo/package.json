    "expo-constants": ">=0",
    "expo-device": ">=0",
    "expo-localization": ">=0",
    "expo-secure-store": ">=0",
    "quanta.tools": ">=0",
    "react": ">=0",
    "react-dom": ">=0",
    "react-native": ">=0",
    "tslib": ">=0"
  },
  "devDependencies": {
    "expo-application": "^6.1.1",
    "expo-constants": "^17.1.1",
    "expo-device": "^7.1.1",
    "expo-localization": "^16.1.1",
